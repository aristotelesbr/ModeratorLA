
<div>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <%= f.label :email, t('login.new.email') %>
  <%= f.email_field :email, autofocus: true %>

  <%= f.label :password, t('login.new.password') %>
  <%= f.password_field :password, autocomplete: "off" %> <br><br>
  <%= f.submit t('helpers.submit.session.new') %>
<% end %>

<%= render "devise/shared/links" %>
</div>
